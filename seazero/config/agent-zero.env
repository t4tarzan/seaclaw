# SeaZero Agent Zero Environment Template
# Copy to ~/.seazero/config/agent-zero.env and edit
#
# LLM Configuration (shared with SeaClaw)
LLM_PROVIDER=openrouter
LLM_MODEL=moonshotai/kimi-k2.5
LLM_API_KEY=
LLM_API_URL=

# Agent Configuration
AGENT_ID=agent-0
AGENT_PORT=8080
LOG_LEVEL=info

# SeaClaw Callback (Agent Zero can request SeaClaw tools)
SEACLAW_CALLBACK_URL=http://host.docker.internal:7432
